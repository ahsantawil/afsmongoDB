<div class="row">
    <div class="col-lg-12">
        <div class="card">
            <div class="card-header">
                <h6 class="card-header-text">Form Data Klaim</h6>
            </div>
            <div class="card-block">
                <form action="" method="POST">
                    <div class="form-row">
                        <div class="form-group col-md-6 ">
                            <label for="uiw">UIW/UID</label>
                            <select name="uiw" id="uiw" class="form-control">
                                <option value="">DISBANTEN</option>
                            </select>
                        </div>
                        <div class="form-group col-md-6">
                            <label for="area">Area</label>
                            <select name="area" id="area" class="form-control">
                                <option value="">CIKOKOL</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label for="no_klaim">Nomor Klaim</label>
                            <input id="no_klaim" name="no_klaim" type="text" class="form-control" placeholder="Masukan Nomor SPB" required>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="no_surat">Nomor Surat</label>
                            <input type="text" name="no_surat" class="form-control" placeholder="Masukan Nomor Surat" required>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="qty">Jumlah Klaim</label>
                            <input type="number" class="form-control" name="qty" required>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group col-md-4">
                            <label for="tgl_kirim">Tahun Produksi</label>
                            <input type="text" class="form-control" name="tgl_kirim" placeholder="Masukan Tahun Produksi" required>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="garansi">Status Klaim</label>
                            <select name="garansi" id="garansi" class="form-control">
                                <option value="">Claim</option>
                                <option value="">DisClaim</option>
                            </select>
                        </div>
                        <div class="form-group col-md-4">
                            <label for="type">Tipe Produk</label>
                            <select name="type" id="type" class="form-control" >
                                <option value="">CSI11</option>
                                <option value="">CSI12</option>
                                <option value="">CSI21</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-row">
                        <div class="form-group">
                            <div class="col-md-4">
                                <label for="sn">Serial Number</label>
                                <input id="sn" name="sn" type="file" class="form-control">
                            </div>
                        </div>
                    </div>
                    <button type="submit" class="btn btn-info waves-effect waves-light m-r-30">Save</button>
                    <button type="reset" class="btn btn-warning waves-effect waves-light m-r-30">Clear</button>
                </form>
            </div>
        </div>
    </div>
</div>

